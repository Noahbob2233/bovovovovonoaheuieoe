{
  "name": "rpnow",
  "version": "2.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href='/'",
    "watch": "ng build --watch",
    "build": "npm run build:ng && npm run build:inject-version",
    "build:ng": "ng build --prod --source-map",
    "build:inject-version": "replace-in-file 'RPNOW_VERSION_TOKEN_DEV' $npm_package_version ./dist/rpnow/index.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "http-server": "http-server ./dist/rpnow -p 4300 --push-state",
    "sentry": "npm run sentry:new && npm run sentry:upload && npm run sentry:finalize",
    "sentry:new": "sentry-cli releases new $npm_package_version",
    "sentry:upload": "sentry-cli releases files $npm_package_version upload-sourcemaps ./dist/rpnow --url-prefix 'https://rpnow.net/client-files/' --validate --no-sourcemap-reference",
    "sentry:finalize": "sentry-cli releases finalize $npm_package_version",
    "remove-map-comments": "sed -i '/^\\(\\/\\/# sourceMappingURL\\)/d' ./dist/rpnow/*.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.6.5",
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.0.0",
    "@angular/cli": "~6.0.5",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/pwa": "^0.8.5",
    "@angular/router": "^6.0.3",
    "@angular/service-worker": "^6.0.3",
    "angulartics2": "^6.1.0",
    "coolstory.js": "^0.1.2",
    "core-js": "^2.4.1",
    "date-fns": "^1.29.0",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.10",
    "marked": "^0.3.12",
    "material-design-icons": "^3.0.1",
    "ngx-color-picker": "^6.2.0",
    "raven-js": "^3.27.0",
    "replace-in-file": "^3.4.2",
    "rxjs": "^6.2.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@sentry/cli": "^1.36.1",
    "@types/crypto-js": "^3.1.38",
    "@types/file-saver": "^1.3.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.0",
    "@types/marked": "^0.3.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "spa-http-server": "^0.9.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "2.7.2"
  }
}
